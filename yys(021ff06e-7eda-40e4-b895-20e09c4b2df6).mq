Function sweepFuben()
    Dim x1=882,y1=200 // x1=882
    Dim x2=124,y2=1807
    Dim i=x1,j=y1
    For i = x1 To x2 Step -100
        For j = y2 To y1 Step -100
            // 从上到下，从右到左
            Tap i,j
        Next
        If isFubenScene() <> True Then
            Exit For
        End If
    Next
    Delay 1200
End Function

Function sweepFuben2()
    Dim intX,intY
    Dim i,j
    ReleaseCapture 
    Dim onceFind=False
    Dim isEnd=False
    For 12    	
        Dim find=False
        For 5
            FindColor 0,0,0,0,"7A622A",0,1,intX,intY
            If intX > -1 And intY > -1 Then 
                Find = True
                onceFind=True
                For j = 0 To 1
                    For i = 1 To 3
                        TouchDown intX+i*100,intY+j*50,i
                    Next
                    Delay 10
                    For i = 1 To 3
                        TouchUp i
                    Next
                    // 让界面停止移动
                    //sweepStop 
                    // If isFubenScene() <> True Then
                    //    Exit For
                    // End If
                Next
            End If
            // 点完界面改变，不管点没点对，都退出
            If isFubenScene() <> True Then 
                Find=True
                onceFind=True
                Exit For
            End If
        Next
        If onceFind = True Then 
            Exit For
        End If
        If find <> True Then 
            //随机左右滑动
            Dim rd=Rnd()*2000
            If rd >=1000 Then     	
                Swipe 216, 1307, 216, 312,1000
            Else
                Swipe 216, 312, 216, 1307 ,1000    
            End If
        End If
        intX = -1
        intY=-1
        // 找箱子
        FindColor 0, 0, 0, 0, "D3F4FF", 0, 1, intX, intY
        If intX > -1 And intY > -1 Then 
            ShowMessage "副本结束"
            isEnd = True
            Touch intX, intY, 100
            Delay 800
            touchUnknow
            Exit For
        End If        
    Next
    If isEnd = True Then     	
        For 5
            FindColor 0, 0, 0, 0, "D3F4FF", 0, 1, intX, intY
            If intX > -1 And intY > -1 Then          
                Touch intX, intY, 100
                Delay 800
                touchUnknow 
                Delay 2000
            End If
        Next
    ElseIf onceFind <> True Then 
        //退出
        Touch 989, 67, 100
        Delay 1000
        // 确定
        Touch 474,1159,100
    End If
End Function

Function ygtzScan()
    Dim x1=905,y1=1638
    Dim x2=190,y2=239
    For 2    
        Delay 1000
        TouchDown x1, y1, 1
        TouchDown x2, y2,2
        TouchMove (x1 + x2) / 2, (y1 + y2) / 2, 2
        Delay 1000
        TouchMove (x1 + x2) / 2, (y1 + y2) / 2, 1
        Delay 1000
        TouchUp 1
        TouchUp 2
    Next
    x1 = 882
    y1 = 200
    x2 = 124
    y2 = 1807
    Dim i=x1,j=y1
    For i = x1 To x2 Step -100
        For j = y2 To y1 Step -100
            // 从上到下，从右到左
            Tap i,j
        Next
        Delay 200
    Next
    Touch 996,94,100
End Function

Function sweepJjtp()
    Dim x1=808,y1=606
    //    809,370|809,886
    //    606,370
    For 3    	
        Dim rx=Int(3*Rnd()),ry=Int(3*Rnd())
        Dim i = x1 - rx * 203
        Dim j = y1 + ry * 513  
        // 选择对手
        Touch i, j, 10
        Delay 500
        // 攻击
        Touch i - 84, j, 10 
        If rx < 1 Then 
            Touch x1-(rx+1)*203-84,j,100
        End If
    Next
End Function

Function sweepJjtp0()	
    Dim x1=808,y1=606
    Dim finded=False
    For i = 0 To 2
        For j = 0 To 2
            Dim tx=x1-i*203
            Dim ty=y1+j*513   	    		
            If CmpColorEx(tx & "|"& ty & "|8CA2B3", 0.95) = 1 Then 
                If Rnd() * 10000 < 7000 Then 
                    // 30%的概率跳过
                    Touch tx, ty, 10
                    Delay 500
                    Touch tx - 84, ty, 10
                    finded = True
                    Exit For
                End If           
            End If
        Next        
        If finded Then 
            Exit For
        End If
    Next
    If finded <> True Then 
        // 随机
        sweepJjtp()
    End If
End Function

