Dim WatchMaxLevel1=False,WatchMaxLevel2=False // 观战式神是否达到满级
// 日常任务执行次数
Dim lastChose=1
Dim yhCount=0
Dim jxCount=0
Dim fbCount=0
Dim hasYgtz13=False
Dim hasYgtz20=False
load 
Randomize
Delay 3000
While True
    ReleaseCapture
    startFight()
    //simpleHelp()
Wend
EndScript

Function load()
    AfterJjtpState=ReadUIConfig("下拉框1")
    JXState = ReadUIConfig("下拉框2")
    yhCount = ReadUIConfig("下拉框御魂")
    jxCount = ReadUIConfig("下拉框觉醒")
    fbCount=ReadUIConfig("下拉框副本")
    Douji12 = ReadUIConfig("Douji12")
    Douji21 = ReadUIConfig("Douji21")
    hasYgtz13 = (ReadUIConfig("Ygtz13")<>True)
    hasYgtz20= (ReadUIConfig("Ygtz20")<>True)
End Function

Function updateTimeState()	
    Dim h=DateTime.Hour(),m=DateTime.Minute(),s=DateTime.Second()
    // 12-13点，21-22点斗技
    If (hasYgtz13=False And h=13) Or (hasYgtz20 = False And h=20) Then 
        ShowMessage "妖怪退治时间"        	
        MainState = 1
        TingzhongState = 3
    ElseIf ((h = 12 And Douji12) Or (h = 21 And Douji21))Then 
        ShowMessage "斗技时间到"
        TimeToDouji = True
        MainState = 1
        TingzhongState = 2
    Else 
        TimeToDouji = False
        MainState = 0
        TingzhongState = 0
    End If    
    updateTimeState=TimeToDouji
End Function

Function reMap()	
    // 斗技什么的高优先级
    If updateTimeState() Then 
        //打斗技        
        MapState = 2
    ElseIf yhCount < 3 Then               	
        ShowMessage "御魂日常未完成，进度："&yhCount&"/3"
        MapState = 1
    ElseIf jxCount < 3 Then                             	
        ShowMessage "觉醒日常未完成，进度："&jxCount&"/3"
        MapState = 0
    ElseIf fbCount < 3 Then 
        ShowMessage "副本日常未完成，进度："&fbCount&"/3"
        MapState = 4
    Else      	
        ShowMessage "状态："&AfterJjtpState
        MapState=AfterJjtpState 
    End If
    If StrengthEnough <> True Then 
        MapState = 3
    End If
End Function

Function simpleHelp()
    //    KeepCapture 
    If isLoad0Scene() Then 
        closeGonggao()        
    ElseIf  isLoadScene() Or isPreLoadScene() Or  isLoadWaitScene() Then
        ShowMessage "登陆加载界面"
        entergame()
    ElseIf isMainScene0() Then	
        Touch 154, 1818, 100
    ElseIf isUpdateScene() Then
        // 新版本更新--确定
        Touch 446,1144,100
    ElseIf isReadyScene() Then
        ShowMessage "准备战斗"
        zhunbei()
        Delay 2000
        setAutoFight()
    ElseIf isWinScene() Then
        ShowMessage "战斗胜利"
        For 3
            If isMapScene() <> True Then         			
                fightAsset 
                Delay 500
            End If
        Next
    ElseIf isFailScene() Then
        ShowMessage "战斗失败"
        fightAsset 
    End If
    //    ReleaseCapture 
    Delay 2000
End Function

Function startFight()
    While True
        reMap()
        KeepCapture 
        If isLoad0Scene() Then 
            closeGonggao()        
        ElseIf  isLoadScene() Or isPreLoadScene() Or  isLoadWaitScene() Then
            ShowMessage "登陆加载界面"
            entergame              
        ElseIf isTingZhongScene() Then
            // 屏幕划到最左边
            swipeLeft()
            //	返回庭院
            Select Case TingzhongState
            Case 3
                ShowMessage "妖怪退治"
                ygtz
            Case 2
                ShowMessage "刷斗技"
                douji
            Case Else
                ShowMessage "返回庭院"
                tingyuan
            End Select
            Delay 2000
        ElseIf isMainScene() Then
            // 屏幕划到最左边
            swipeLeft()
            getStrengthGift()
            //探索
            Select Case MainState
            Case 1
                ShowMessage "进入町中"
                tingzhong 
            Case Else
                ShowMessage "探索"
                ts 
                tingyuan
            End Select  
        ElseIf isMainScene0() Then
            // 登陆加载
            Touch 154, 1818, 100
        ElseIf isInvited() Then
            If ConFailTimes < 2 Then 
                // 连败不接受邀请
                actIvt()
            End If 
        ElseIf isMapScene() Then
            ReleaseCapture 
            openBaoxiang 
            openShiju
            Dim flag=False
            For 6
                If ConFailTimes >= 2 Then 
                    // 连败不接受邀请
                    Exit For
                ElseIf isInvited() Then 
                    actIvt 
                    flag=True
                Else 
                    Delay 500
                End If
            Next
            reMap()
            If flag <> True Then 
                Select Case MapState
                Case 0
                    ShowMessage "刷觉醒"
                    jxcl 
                Case 2
                    ShowMessage "返回大厅"
                    mapToMain()
                Case 3
                    ShowMessage "刷结界突破"
                    jjtp()
                Case 4
                    ShowMessage "刷副本"
                    // 重置
                    //For 3
                    //    Swipe 171, 1761, 702, 1761, 500	
                    //Next
                    //Delay 1500
                    // 行高177
                    //Swipe 770, 1761, 171, 1761, 500                    
                    //Delay 1000
                    // 12章
                    //Touch 440,1719,100
                    // 11章
                    //Touch 640, 1719, 100
                    // 13章
                    // Touch 276,1800,100
                    
                    //---------------------------------
                    // 15章――倒数第五章
                    //Touch 858,1757,100
                    // 16章――倒数第四章
                    Touch 728,1776,100
                    // 17章――倒数第三章
                    // Touch 559,1764,100
                    // 18章――倒数第二章
                    // Touch 365,1743,100
                    // 番外章――倒数第一章
                    // Touch 226,1788,100
                Case Else
                    ShowMessage "打御魂"
                    yh()
                End Select                
                Delay 1500
            End If
        ElseIf isYhSelectScene() Then
            lastChose=1
            yhCount = yhCount + 1
            If YHState = 0 Then 
                // 选择八岐大蛇御魂
                Touch 469, 557, 100
            Else             
                // 选业原火
                Touch 469,1339,100
            End If
        ElseIf isJxSelectScene() Then
            lastChose = 0
            jxCount=jxCount+1
            Dim rd=4*Rnd()
            If JXState <> 0 Then 
                rd=JXState-1
            End If            
            If rd >= 3 Then
                // 选择觉醒天雷鼓
                jxtlg 
            ElseIf rd >= 2 Then 
                jxsll   
            ElseIf rd >= 1 Then
                jxfzf
            Else
                jxyhl
            End If
        ElseIf isYyhScene() Then
            ShowMessage "挑战业原火"
            // 业原火
            For 2
                Touch 336,1432,100
            Next
            Delay 300
            // 点关闭
            Touch 894,1573,100
        ElseIf isReadyScene() Then
            ShowMessage "准备战斗"
            If (WatchMaxLevel1 Or WatchMaxLevel2) And MapState=4 Then 
                // 更换式神
                ShowMessage "更换式神"
                changeShiShen()          	
            End If
            Delay 300
            zhunbei()
            Delay 2000
            setAutoFight()
        ElseIf isWinScene() Then
            ShowMessage "战斗胜利"
            Delay 1500
            KeepCapture
            If isLevelMax1() Then             
                ShowMessage "观战式神1 经验满"
                WatchMaxLevel1=True
            End If
            If isLevelMax2() Then             	
                ShowMessage "观战式神2 经验满"
                // 式神经验满级
                WatchMaxLevel2=True
            End If
            // 重置连败次数
            ConFailTimes = 0
            ReleaseCapture
            For 7
                // If isMapScene() <> True Then         			
                fightAsset 
                Delay 300
                // End If
            Next
        ElseIf isRewardScene() Then
            ShowMessage "领取奖励"
            For 3
                If isMapScene() <> True Then         			
                    fightAsset 
                    Delay 300
                End If
            Next
        ElseIf isFailScene() Then
            ShowMessage "战斗失败"
            // 连败次数加1
            ConFailTimes=ConFailTimes+1
            fightAsset 
        ElseIf isCancleOKScene() Then
            cancel()            
        ElseIf isBuyStrengthScene() Then
            ShowMessage "体力不足！"
            closeStrength 
            StrengthEnough=False
            MapState=3 //打结界突破去
            Delay 5000
            //buyStrength()
        ElseIf isXuanshangIvtScene() Then
            // 悬赏接受
            // xuanshangAct()
            // 拒绝
            ShowMessage "拒绝悬赏任务"
            xuanshangRej()
        ElseIf isCreateRoomScene() Then
            crt()
        ElseIf isInTeamScene() Then
            ReleaseCapture 
            Dim autoStop=False
            Dim count=0
            For 48
                KeepCapture 
                If ConFailTimes < 2 And isInVited() Then 
                    // 连败不接受邀请
                    actIvt 
                ElseIf canJoin() Then
                    joinTeam()
                ElseIf isInTeamScene()
                    freshTeam 
                    count=count+1
                    Delay 500
                Else 
                    autoStop=True
                    Exit For                
                End If
                ReleaseCapture
            Next
            // 若刷完72次没有找到合适的房间
            If autoStop <> True Or count >= 48 Then 
                ShowMessage "没有房间，自己创建"
                createRoom()
            End If
            
        ElseIf isTeamScene() Then
            // 组队
            team 
            // 挑战
            // challange
        ElseIf isRoomScene() Then
            If canRoomStart() <> True Then             	
                ShowMessage "房间内-尚未开始"
                Delay 1000
            Else 
                roomStart()
            End If
        ElseIf isFubenScene() Then
            ShowMessage "副本内"
            sweepFuben2
            KeepCapture
            If isFubenSelectScene() Then         	
                // 扫描结束后关闭误点出来的其他副本界面
                Tap 862, 1586
            End If
        ElseIf isFubenSelectScene() Then
            lastChose = 4
            fbCount=fbCount+1
            ShowMessage "副本探索"
            // 困难
            Touch 744, 719, 100
            // 普通
            // Touch 744,506,100
            Delay 500
            fbTansuo 
        ElseIf isJjtpScene() Then
            ShowMessage "结界突破"
            If ConFailTimes >= 2 Then 
                ShowMessage "连败" & ConFailTimes & "次，重新刷新！"
                Touch 266, 1570, 100
                Delay 1000
                Touch 437, 1141, 100
                Delay 1000
                ConFailTimes=0
            End If
            If canJjtpExit() Then 
                ShowMessage "没有突破券了"
                windExit 
                // 默认打完突破是有体力的
                StrengthEnough=True
            Else 
                sweepJjtp0()
            End If
        ElseIf isJjtpCover() Then
            ShowMessage "结界灰"
            Touch 170, 1078, 100
        ElseIf isDoujiScene() Then
            ShowMessage "刷斗技"
            Touch 181, 1684, 100
            // 防止不在时间内
            Delay 2000
            windExit 
        ElseIf isYgtzScene() Then
            ygtzScan 
            Delay 1000
        ElseIf isYgtzStartScene() Then
            ShowMessage "妖怪退治挑战"
            Touch 290, 1416, 100
            Delay 1000
            
            If DateTime.Hour() = 13 Then 
                hasYgtz13 = True
            ElseIf DateTime.Hour() = 20 Then
                hasYgtz20=True
            End If
        ElseIf isFighting() Then
            ShowMessage "正在战斗中"
            setAutoFight 
            If MapState = 3 Then 
                // 打结界，集火
                ShowMessage "集火"
                Touch 710,464,300
                Touch 662,710,300               
                Touch 696,958,300
                Touch 710,1206,300
                Touch 732,1421,300
            End If
            Dim cal=TickCount()\TextDelay
            If cal > textCount Then             
                textCount = cal
                If MapState = 2 Then 
                    sendText "挂机了"
                ElseIf MapState < 3 Then                	
                    sendText "挂机了"
                End If
            Else 
                Delay 2000
            End If
        ElseIf isYgfxScene() Then
            Touch 260, 1522, 100
            Delay 200
            Touch 927,1704,100
            Delay 1000
            Touch 322, 965, 100
            Delay 1000
        ElseIf isShareScene() Then
            ShowMessage "有稀有物品掉落！！！"
            // Touch 936,1756,100
            Delay 2000
        ElseIf isKeyBoardScene() Then
            // 送信
            ShowMessage "送信"
            Touch 759, 704, 100
            Delay 1000
            // 送信
            songxin()
        ElseIf isHdjlScene() Then
            // 获得奖励界面
            ShowMessage "获得奖励"
            //  Touch 732,1448,100
            windExit 
        ElseIf isDisConnectScene() Then
            // 断开服务器，需要重连
            ShowMessage "连接断开，重新连接"
            Touch 476,962,100
        ElseIf isUpdateScene() Then
            // 新版本更新--确定
            Touch 446, 1144, 100
        ElseIf isNetErrorScene() Then
            // 网络错误
            Touch 365, 965, 100
            Delay 1000
        ElseIf disConnectEndScene() Then
            // 断线期间战斗已结束
            Touch 450, 961, 100
            Delay 1000
        ElseIf isChatScene() Then
            songxin 
            Delay 1000
            ShowMessage "关闭对话"
            Touch 1003, 799, 100
        ElseIf canClose() Then
            ShowMessage "未识别的可关闭窗口，尝试关闭"
            windExit 
            Delay 2000
        ElseIf canReturn() Then
            ShowMessage "未识别的可返回界面，尝试返回"
            Touch 994, 89, 100
            Delay 2000
        Else
            ShowMessage "未知界面"
            touchUnknow
            Delay 2000
        End If
        ReleaseCapture 
        Delay 1000
    Wend 
End Function

Function setAutoFight()
    ReleaseCapture
    For 5
        If isNotAuto() Then 
            ShowMessage "设置自动战斗"
            Touch 92, 87, 100
            Delay 2000
        End If
    Next
    KeepCapture
End Function

